APP_NAME=OQLook
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost/OQLook
ASSET_URL=http://localhost/OQLook

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=oqlook
DB_USERNAME=oqlook
DB_PASSWORD=secret

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=file
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

OQLIKE_USE_QUEUE=false
OQLIKE_HTTP_TIMEOUT=30
OQLIKE_HTTP_RETRIES=3
OQLIKE_HTTP_RETRY_MS=250
OQLIKE_HTTP_VERIFY_TLS=true
# OQLIKE_HTTP_CA_PATH=
OQLIKE_ITOP_DISCOVERY_ROOT_CLASSES=cmdbAbstractObject,CMDBAbstractObject,CMDBObject
OQLIKE_RATE_LIMIT_MS=150
OQLIKE_DEFAULT_THRESHOLD_DAYS=365
OQLIKE_MAX_SAMPLES=25
OQLIKE_PAGE_SIZE=250
OQLIKE_MAX_FULL_RECORDS_PER_CLASS=5000
OQLIKE_MAX_FULL_RECORDS_WITHOUT_DELTA=5000
OQLIKE_ISSUE_OBJECTS_MAX_FETCH=5000
OQLIKE_OBJECT_ACK_ENABLED=true
OQLIKE_OBJECT_ACK_MAX_VERIFICATIONS_PER_ISSUE=250
OQLIKE_ADMIN_PACK_ENABLED=true
OQLIKE_ADMIN_PACK_MAX_RECORDS_PER_CHECK=2000
OQLIKE_ADMIN_PACK_ORPHAN_CHECK_ENABLED=true
OQLIKE_ADMIN_PACK_ORPHAN_MAX_RECORDS=1200
OQLIKE_ADMIN_PACK_ORPHAN_MAX_ATTRIBUTES=6
OQLIKE_ADMIN_PACK_ORPHAN_PRIORITY_FIELDS=org_id,location_id,parent_id,functionalci_id,contact_id,owner_id,team_id
OQLIKE_ADMIN_PACK_STALE_WITHOUT_OWNER_ENABLED=true
OQLIKE_ADMIN_PACK_CLASSIFICATION_DRIFT_ENABLED=true
# OQLIKE_ADMIN_PACK_EXCLUDED_CLASSES=ActionEmail,ActionWebhook
OQLIKE_ADMIN_PACK_OWNERSHIP_FIELDS=owner_id,team_id,agent_id,manager_id,contact_id,caller_id
OQLIKE_ADMIN_PACK_CLASSIFICATION_FIELDS=org_id,location_id
OQLIKE_ADMIN_PACK_PLACEHOLDER_TERMS=test,tmp,todo,tbd,sample,dummy,unknown,n/a,na,xxx,to_define
OQLIKE_ADMIN_PACK_PLACEHOLDER_MIN_TERM_LENGTH=2
OQLIKE_MAX_DUPLICATE_SCAN_RECORDS=10000
OQLIKE_DISCOVERY_SCAN_LIMIT=400
OQLIKE_MAX_CONNECTOR_CLASSES=800
OQLIKE_CONNECTOR_MEMORY_GUARD_RATIO=0.70
OQLIKE_CONNECTOR_MEMORY_HARD_STOP_RATIO=0.92
# OQLIKE_CONNECTOR_TEMP_DIR=

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
